 from flask import Flask, jsonify
    
    app = Flask(__name__)
    
    @app.route('/data')
    def get_data():
        data = {
            "name": "Example API",
            "version": "1.0",
            "message": "Hello from PythonAnywhere!",
            "items": [
                {"id": 1, "value": "item A"},
                {"id": 2, "value": "item B"}
            ]
        }
        return jsonify(data)
    
    if __name__ == '__main__':
        app.run(debug=True)
